{
  "name": "dom-helpers",
  "version": "2.0.0",
  "description": "High-performance vanilla JavaScript DOM utilities with intelligent caching - Elements Helper (ID access) + Collections Helper (class/tag/name access) + Selector Helper (querySelector with caching)",
  "main": "dist/dom-helpers.bundle.js",
  "module": "dist/dom-helpers.esm.js",
  "unpkg": "dist/dom-helpers.min.js",
  "jsdelivr": "dist/dom-helpers.min.js",
  "types": "dist/dom-helpers.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dom-helpers.esm.js",
      "require": "./dist/dom-helpers.js",
      "browser": "./dist/dom-helpers.min.js"
    },
    "./elements": {
      "import": "./dist/elements.esm.js",
      "require": "./dist/elements.js",
      "browser": "./dist/elements.min.js"
    },
    "./collections": {
      "import": "./dist/collections.esm.js",
      "require": "./dist/collections.js",
      "browser": "./dist/collections.min.js"
    },
    "./selector": {
      "import": "./dist/selector.esm.js",
      "require": "./dist/selector.js",
      "browser": "./dist/selector.min.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "src/",
    "docs/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node scripts/build-all.js",
    "build:all": "node scripts/build-all.js",
    "build:individual": "npm run build:elements && npm run build:collections",
    "build:elements": "node scripts/build.js elements",
    "build:collections": "node scripts/build.js collections",
    "build:combined": "node scripts/build.js combined",
    "build:min": "node scripts/minify.js",
    "test": "echo \"Tests coming soon\" && exit 0",
    "prepublishOnly": "npm run build",
    "dev": "npm run build && node -e \"console.log('Build completed. Check dist/ folder.')\"",
    "clean": "node -e \"const fs=require('fs'); if(fs.existsSync('dist')) fs.rmSync('dist', {recursive:true}); console.log('Cleaned dist/ folder');\""
  },
  "keywords": [
    "dom",
    "elements",
    "collections",
    "selector",
    "javascript",
    "vanilla-js",
    "performance",
    "caching",
    "mutation-observer",
    "browser",
    "frontend",
    "utility",
    "getElementById",
    "getElementsByClassName",
    "getElementsByTagName",
    "getElementsByName",
    "querySelector",
    "querySelectorAll",
    "proxy",
    "cdn"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/giovanni1707/elements-helper.git"
  },
  "bugs": {
    "url": "https://github.com/giovanni1707/elements-helper/issues"
  },
  "homepage": "https://github.com/giovanni1707/elements-helper#readme",
  "devDependencies": {
    "terser": "^5.44.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "cdn": {
    "jsdelivr": {
      "combined": "https://cdn.jsdelivr.net/npm/dom-helpers@2/dist/dom-helpers.min.js",
      "elements": "https://cdn.jsdelivr.net/npm/dom-helpers@2/dist/elements.min.js",
      "collections": "https://cdn.jsdelivr.net/npm/dom-helpers@2/dist/collections.min.js",
      "selector": "https://cdn.jsdelivr.net/npm/dom-helpers@2/dist/selector.min.js"
    },
    "unpkg": {
      "combined": "https://unpkg.com/dom-helpers@2/dist/dom-helpers.min.js",
      "elements": "https://unpkg.com/dom-helpers@2/dist/elements.min.js",
      "collections": "https://unpkg.com/dom-helpers@2/dist/collections.min.js",
      "selector": "https://unpkg.com/dom-helpers@2/dist/selector.min.js"
    }
  }
}
